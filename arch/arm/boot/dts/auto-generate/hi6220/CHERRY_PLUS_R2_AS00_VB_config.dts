/dts-v1/;
/memreserve/ 0x07000000 0x00400000;
/include/ "CHERRY_PLUS_R2_UL00_VB_config.dtsi"
/include/ "CHERRY_PLUS_R2_AS00_VB_pin.dtsi"
/include/ "CHERRY_PLUS_R2_AS00_VB_gpio_logic.dtsi"
/include/ "CHERRY_PLUS_R2_AS00_VB_pmic.dtsi"
/include/ "CHERRY_PLUS_R2_AS00_VB_charger.dtsi"
/ {
	model = "Hisilicon Hi6210 Development Board";
	compatible = "hisilicon,hi6210";
	hisi,boardid = <0 7 9 0>;
	hisi,boardname = "CHERRY_PLUS_R2_AS00_VB";
        hisi,modem_id = <0x3DB01500>;
	hisi,product_name = "CHERRY_PLUS_AS00";
    
    boardinfo{
	factory_product_name = "HL4CHRPLSL12MAG";
	normal_product_name = "Che2-L12";
	};

    amba {
	i2c2: i2c@f7102000 {
			st_lis3dh_acc:acc@18{
				axis_map_x=<0>;
				axis_map_y=<1>;
				axis_map_z=<2>;
				negate_x=<0>;
				negate_y=<1>;
				negate_z=<1>;
				status = "ok";
			};
			rohm_kc023_acc:acc@1E{
				axis_map_x=<0>;
				axis_map_y=<1>;
				axis_map_z=<2>;
				negate_x=<0>;
				negate_y=<1>;
				negate_z=<1>;
				status = "ok";
			};
		};
	};

	bcm_wifi {
		wifi_nvram_name = "/vendor/firmware/nvram_CHERRY_PLUS_AS00.txt";
		status = "ok";
	};
};
